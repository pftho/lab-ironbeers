<div class="random-beer">

    {{#each randomBeerFromAPI}}


    {{> beerpartial this}}

    <div class="extra-info">
        <h3>Food Pairing: </h3>
        <ul>
            {{#each this.food_pairing}}
            <li> {{this}} </li>
            {{/each}}

        </ul>

        <h3>Bewers Tips: </h3>
        <p> {{this.brewers_tips}}</p>


    </div>
    {{/each}}

</div>